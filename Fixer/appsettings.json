{
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TmdbApi": {
    "ApiKey": "secret",
    "ApiKeyRequestParamName": "api_key",
    "BaseUrl": "https://api.themoviedb.org/3",
    "PageRequestParamName": "page"
  },
  "TmdbCacheUpdater": {
    "MaxPageNumber": 3
  },
  "TmdbDataRefresherService": {
    "UpdateTaskDelayMinutes": 1
  }
}
